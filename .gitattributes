# Windows executables
*.exe filter=lfs diff=lfs merge=lfs -text
windows/dist/StreamerUploader.exe filter=lfs diff=lfs merge=lfs -text

# Linux executables (no extension)
linux/dist/StreamerUploader filter=lfs diff=lfs merge=lfs -text

# macOS executables (no extension)
macos/dist/StreamerUploader filter=lfs diff=lfs merge=lfs -text

# Alternative: Track all executables in dist directories
# dist/**/StreamerUploader filter=lfs diff=lfs merge=lfs -text
